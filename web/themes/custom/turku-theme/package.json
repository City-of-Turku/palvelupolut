{
  "name": "turku-drupal-theme",
  "version": "1.0.0",
  "description": "Storybook development + Webpack Build + Drupal 9 theme",
  "keywords": [
    "component library",
    "design system",
    "drupal",
    "pattern library",
    "storybook",
    "styleguide"
  ],
  "author": "Evan Willhite <evan@fourkitchens.com>",
  "license": "MIT",
  "dependencies": {
    "add-attributes-twig-extension": "^0.1.0",
    "autoprefixer": "^9.8.6",
    "bem-twig-extension": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "file-loader": "^6.1.0",
    "fs": "^0.0.1-security",
    "glob": "^7.1.4",
    "imagemin-webpack-plugin": "^2.4.2",
    "js-yaml-loader": "^1.2.2",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "normalize.css": "^8.0.1",
    "photoswipe": "^4.1.3",
    "postcss-custom-properties": "^9.1.1",
    "postcss-loader": "^4.2.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.2.1",
    "svg-sprite-loader": "^5.2.1",
    "twig": "git+https://github.com/dbruvers/twig.js",
    "twig-drupal-filters": "^3.1.0",
    "twig-loader": "git+https://github.com/fourkitchens/twig-loader.git",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^5.7.3"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint ./components",
    "a11y": "npm run build-storybook && ./scripts/a11y.js -r",
    "storybook": "start-storybook --ci -s ./dist,./images -p 6006",
    "build-storybook": "build-storybook -s ./dist,./images -o storybook-static",
    "webpack": "webpack --watch --config ./webpack/webpack.dev.js --display-error-details",
    "build": "webpack --config ./webpack/webpack.prod.js --display-error-details",
    "develop": "concurrently --raw \"npm run webpack\" \"npm run storybook\"",
    "format": "prettier --write \"**/*.{js,yml,scss,md}\"",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@storybook/addon-a11y": "6.1.10",
    "@storybook/addon-actions": "6.1.10",
    "@storybook/addon-links": "6.1.10",
    "@storybook/addon-toolbars": "^6.2.9",
    "@storybook/addons": "6.1.10",
    "@storybook/react": "^6.1.20",
    "@storybook/storybook-deployer": "^2.8.7",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.0",
    "chalk": "^4.0.0",
    "commitizen": "^4.2.4",
    "concurrently": "^5.3.0",
    "core-js": "^3.8.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "pa11y": "^5.3.0",
    "prettier": "2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.0.0",
    "yaml": "^1.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
