{% set list__base_class = menu_class %}
{% set list__modifiers = menu_modifiers %}
{% set list__blockname = menu_blockname %}

{# List #}
{% extends "@atoms/lists/list-unordered.twig" %}
  {% block list_content %}
    {% for item in items %}
      {% include "@molecules/navigation/_menu-item.twig" with {
        list_item__base_class: item_base_class,
        list_item__modifiers: item_modifiers,
        list_item__blockname: item_blockname,
      } %}
    {% endfor %}
  {% endblock %}
