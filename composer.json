{
    "name": "wunderio/client-fi-turku-service-paths",
    "description": "Service Paths project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "fonticonpicker/fonticonpicker--fonticonpicker",
                "version": "3.1.1",
                "type": "drupal-library-dist",
                "dist": {
                    "url": "https://github.com/fontIconPicker/fontIconPicker/releases/download/v3.1.1/fontIconPicker.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "leaflet/leaflet",
                "version": "1.0.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/Leaflet/Leaflet/archive/v1.0.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "furf/jquery-ui-touch-punch",
                "version": "dev-master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.11",
        "drupal/admin_toolbar": "^3.1.1",
        "drupal/backup_migrate": "^5.0.2",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/breadcrumb_manager": "^1.3",
        "drupal/chosen": "^4.0",
        "drupal/components": "^3.0@beta",
        "drupal/config_filter": "^2.4",
        "drupal/config_ignore": "^2.3",
        "drupal/config_perms": "^2.1",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/ctools": "^4.0",
        "drupal/devel": "^5.0",
        "drupal/devel_kint_extras": "^1.1",
        "drupal/devel_php": "^1.5",
        "drupal/double_field": "^4.0",
        "drupal/emulsify_twig": "^4.0",
        "drupal/eva": "^3.0",
        "drupal/field_group": "^3.2",
        "drupal/focal_point": "^2.0",
        "drupal/fontawesome": "2.x-dev",
        "drupal/geocoder": "^3.20 || ^4.0",
        "drupal/geofield": "^1.44",
        "drupal/google_tag": "^2.0",
        "drupal/honeypot": "^2.1.1",
        "drupal/hreflang": "^1.9",
        "drupal/leaflet": "^10.0",
        "drupal/leaflet_more_maps": "^2.1.2",
        "drupal/linkit": "^6.0@beta",
        "drupal/material_icons": "^2.0",
        "drupal/menu_admin_per_menu": "^1.5",
        "drupal/menu_trail_by_path": "^2.0",
        "drupal/metatag": "^1.21",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_scheduler": "1.0-rc1",
        "drupal/migrate_tools": "^6.0",
        "drupal/monolog": "^2.2",
        "drupal/paragraphs": "^1.14",
        "drupal/paragraphs_asymmetric_translation_widgets": "1.x-dev",
        "drupal/password_policy": "^4.0",
        "drupal/pathauto": "^1.11",
        "drupal/rdf": "^2.0",
        "drupal/roleassign": "^2.0",
        "drupal/seven": "^1.0",
        "drupal/simplei": "^2.1",
        "drupal/stable": "^2.0",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/token": "^1.11",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/transliterate_twig": "^1.0",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.1.3",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/view_mode_page": "^4.0.1",
        "drush/drush": "^11",
        "erusev/parsedown": "^1.7.4",
        "fonticonpicker/fonticonpicker--fonticonpicker": "^3.1.1",
        "furf/jquery-ui-touch-punch": "dev-master",
        "geocoder-php/nominatim-provider": "^5.6",
        "harvesthq/chosen": "^1.8.7",
        "kint-php/kint": "^3.3",
        "leaflet/leaflet": "^1.0.2",
        "mglaman/composer-drupal-lenient": "^1.0",
        "vlucas/phpdotenv": "^5.4.1",
        "webflo/drupal-finder": "^1.2.2",
        "wunderio/drupal-ping": "^2.3.3",
        "wunderio/updates_log": "^2.4"
    },
    "require-dev": {
        "vimeo/psalm": "^4.26",
        "wunderio/code-quality": "^2.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "koodimonni/composer-dropin-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "mglaman/composer-drupal-lenient": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "process-timeout": 600
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/libraries/{$name}/dist": [
                "type:drupal-library-dist"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/core": {
                "Entity view/form mode formatter/widget settings have no translation UI": "https://www.drupal.org/files/issues/2020-12-04/2546212-168.patch",
                "CKEditor 5 isn't respecting field widgets row settings | https://www.drupal.org/project/drupal/issues/3241295": "https://www.drupal.org/files/issues/2023-05-08/3241295-d10.0.9-108.patch",
                "CKEditor 5 should not grow to infinite height | https://www.drupal.org/project/drupal/issues/3273755": "https://www.drupal.org/files/issues/2023-05-31/3273755-64-CK5_add_height.patch"
            },
            "drupal/devel": {
                "Brings back the Available Methods & Properties tabs for Kint dumps â€” Issue #221": "https://raw.githubusercontent.com/politsin/snipets/master/patch/kint.patch"
            },
            "drupal/stage_file_proxy": {
                "It should be possible to disable the module, but still have it installed": "https://www.drupal.org/files/issues/2019-11-18/disable_fetching-2864016-26.patch"
            },
            "drupal/migrate_scheduler": {
                "Make the configuration available to set/update from the admin UI": "https://www.drupal.org/files/issues/2021-04-14/migrate_scheduler_2973261_6.patch",
                "When using Migrate Plus, update the Last imported date stamp": "https://www.drupal.org/files/issues/2021-05-11/migrate_scheduler-migrate_plus_imported_date-3213539-2.patch",
                "Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-06-16/migrate_scheduler.1.x-dev.rector.patch"
            },
            "drupal/config_ignore": {
                "Support for export filtering via Drush": "https://www.drupal.org/files/issues/2021-01-04/config_ignore_2857247-61.patch"
            },
            "drupal/migrate_tools": {
                "Do not roll back everything with --sync option": "https://www.drupal.org/files/issues/2023-05-16/sync-id-hash-3104268-21.patch"
            },
            "drupal/paragraphs": {
                "Core 9.5.0 causing broken-looking styling (Legacy widget, with Seven) | https://www.drupal.org/project/paragraphs/issues/3331203": "https://www.drupal.org/files/issues/2023-01-06/3331203-2.patch"
            },
            "drupal/paragraphs_asymmetric_translation_widgets": {
                "EntityStorageException on paragraphs_asymmetric_translation_widgets_entity_translation_delete entityQuery | https://www.drupal.org/project/paragraphs_asymmetric_translation_widgets/issues/3358805": "https://www.drupal.org/files/issues/2023-05-08/paragraphs_asymmetric_translation_widgets-entity-query-access-3358805-2.patch"
            },
            "drupal/view_mode_page": {
                "Drupal 10 compatibility | https://www.drupal.org/project/view_mode_page/issues/3356400": "https://www.drupal.org/files/issues/2023-05-30/3356400-drupal-10-compatibility-6.patch"
            }
        },
        "dropin-paths": {
            "web/": [
                "type:web-dropin",
                "package:wunderio/drupal-ping:_ping.php"
            ]
        },
        "drupal-lenient": {
            "allowed-list": ["drupal/migrate_scheduler"]
        }
    }
}
